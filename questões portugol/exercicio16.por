programa
{
    funcao inicio()
    {
        inteiro i, j

        para (i = 0; i <= 10; i = i + 1)
        {
            para (j = 0; j <= 10; j = j + 1)
            {
                inteiro resultado = i * j
                escreva("tab ", i, " x ", j, " = ", resultado, "\n")
            }
        }
    }
}
